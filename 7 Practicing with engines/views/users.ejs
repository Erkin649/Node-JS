<%- include('includes/head.ejs') %>
    <h1>User</h1>
    <% if (users.length > 0) {%>
        <ul>
            <% users.forEach( (user) => { %>
                <li><%= user.name %></li>
            <% })%>
        </ul>
    <% } else { %>
        <p>No users to show!</p>
    <% } %>
<%- include('includes/end.ejs') %>